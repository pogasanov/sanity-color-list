{
  "name": "@pogasanov/sanity-plugin-color-list",
  "version": "1.2.0",
  "description": "Displays a list of colors",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/pogasanov/sanity-color-list#readme",
  "bugs": {
    "url": "https://github.com/pogasanov/sanity-color-list/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pogasanov/sanity-color-list.git"
  },
  "license": "MIT",
  "author": "Kim Bj√∂rkman <hello@bjorkman.kim>",
  "exports": {
    ".": {
      "types": "./lib/src/index.d.ts",
      "source": "./src/index.ts",
      "import": "./lib/index.esm.js",
      "require": "./lib/index.js",
      "default": "./lib/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "source": "./src/index.ts",
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "types": "./lib/src/index.d.ts",
  "files": [
    "src",
    "lib",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "scripts": {
    "prebuild": "npm run clean && plugin-kit verify-package --silent && pkg-utils",
    "build": "pkg-utils build --strict",
    "clean": "rimraf lib",
    "compile": "tsc --noEmit",
    "format": "prettier src -w",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.1",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^0.37.22",
    "@types/styled-components": "^5.1.26"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@sanity/pkg-utils": "^1.18.0",
    "@sanity/plugin-kit": "^2.1.16",
    "@sanity/semantic-release-preset": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "react": "^18",
    "rimraf": "^3.0.2",
    "sanity": "^3.0.0",
    "semantic-release": "^20.1.1",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3.0.0",
    "styled-components": "^5.3.6"
  },
  "engines": {
    "node": ">=14"
  }
}
